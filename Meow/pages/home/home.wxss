/* pages/home/home.wxss */
.background {
    display: flex;
    flex-direction: column;
  }
  
  .background-image {
    width: 100%;
    height: 500px;
  }
  
  .background-content {
    position: absolute;
    z-index: 1;
  }
  .HomeText{
    width: 720rpx;
    height: 220rpx;
    color: rgb(97, 59, 11);
    margin: 20rpx;
    font-size: 40px;
    font-weight: 800;
    font-family:"宋体";
    align-items: center;
    justify-content: center;
    display:flex;
    border-radius: 20%;
  }
  .in-image{
      justify-content: center;
      width: 352px;
      height: 300px;
      align-items: center;
      padding: 13px;
      left: 400px;
      border-radius: 600rpx;
  }